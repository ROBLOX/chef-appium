<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Debug</key>
	<true/>
	<key>EnvironmentVariables</key>
	<dict>
		<key>PATH</key>
		<string>/usr/local/bin:/usr/local/sbin/:/usr/sbin:/usr/bin:/bin/sbin</string>
	</dict>
	<key>KeepAlive</key>
	<dict>
		<key>SuccessfulExit</key>
		<false/>
	</dict>
	<key>Label</key>
	<string>io.appium.server</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/appium</string>
		<string>--session-override</string>
		<string>--log-level</string>
		<string>warn</string>
		<string>--log-no-colors</string>
		<string>--log-timestamp</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string><%= @user_home %>/Library/Logs/appium.err.log</string>
	<key>StandardOutPath</key>
	<string><%= @user_home %>/Library/Logs/appium.log</string>
	<key>WorkingDirectory</key>
	<string><%= @user_home %></string>
</dict>
</plist>
